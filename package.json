{
  "name": "get-med-go",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "pre-commit": "yarn lint --fix",
    "local": "rm -rf .next && yarn build && next start -p 3000",
    "start:prod": "docker-compose --env-file ./.env.production.local down && docker-compose --env-file ./.env.production.local up --build",
    "start:dev": "docker-compose --env-file ./.env.production.local -f docker-compose-dev.yml down && docker-compose -f docker-compose-dev.yml --env-file ./.env.production.local up --build"
  },
  "dependencies": {
    "framer-motion": "^6.5.1",
    "husky": "^8.0.1",
    "next": "^12.2.3",
    "nprogress": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0",
    "sharp": "^0.30.7",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "18.6.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.3",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.7",
    "typescript": "4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
